<VBox prefHeight="400.0" prefWidth="761.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="marchal.gabriel.tl.ConfigurarUsuariosController">
    <children>
